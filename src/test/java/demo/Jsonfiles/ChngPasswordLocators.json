{
	"loginScreen": {
		"buttons": {
			"continueAsGuest": ".highlight-light-button",
			"login": ".highlight-dark-button",
			"search": ".location-Submit-Btn"
		},
		"fields": {
			"Email": "input[id='Email']",
			"password": "input[id='password']",
			"Location": "input[id=location]"
		}
	},
	"locationSearchScreen": {
		"fields": {
			"selectedItem": ".list_body_item",
			"selectedStore": ".store-selection-store-name"
		}
	},
	"Changepswrd": {
		"Profile": {
			"profileIcon": "//div[@class='menu-inner-container']/div[2]",
			"PreferenceTab": "//tr[@class='tabs-items']/td[3]/div/p",
			"Change_Password": "//div[@class='vertical-align-text']/div"
		}
	},
	"ChangepswrdForm": {
		"Password": {
			"CurrentPassword": "//div[@class='inner-body-container']/ion-row/ion-col/div/div/input",
			"NewPassword": "//div[@class='inner-body-container']/ion-row[2]/ion-col/div/input",
			"ConfirmPassword": "//div[@class='inner-body-container']/ion-row[3]/ion-col/div/input",
			"SAVEButton": "//button[text()='save']"
		},
		"ErrorMessages": {
			"Validations": {
				"Newpassword" : "//div[@class='inner-body-container']/ion-row[2]/ion-col/p/span",
				"confirmpassword": "//div[@class='inner-body-container']/ion-row[3]/ion-col/p/span",
				"popup": "//div[@class='alert-description-container']/p",
				"PopupCloseButton" : "//button[@id='closebutton']"
			}
		}
	}
}